fn strtok(Array<string> strings, string delim) {
  string output = ""
  int length = length(strings)
  for (int i = 0; i < length - 1; i++) {
    output += strings[i] + delim
  }
  output += strings[length-1]
  return output
}

fn strtok(Array<string> strings) {
    return strtok(strings, ",")
}

