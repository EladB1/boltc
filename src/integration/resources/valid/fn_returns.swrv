fn returns(int i): int {
    if (i % 2 == 0) {
        if (i < 0)
            return returns(-i);
        else if (i == 0)
            return i;
        return i / 2;
    }
    else {
        return returns(i-1);
    }
}

fn complex(int i): boolean {
    if (i <= 1) {
        if (i == 1)
            return true;
        else if (i == 0)
            return true;
        else
            return false;
    }
    else
        return false;
}

