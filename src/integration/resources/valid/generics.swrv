prototype first(Array<generic> array): generic {
    if (length(array) == 0)
        return null;
    generic frst = array[0];
    removeIndex(array, 0);
    return frst;
}

fn main() {
    Array<double> doubles[3] = {-9.8, 3.14, 6.67 * 10 ** -11};
    double f = first(doubles);
    string s = first({"hello", "world"});
}